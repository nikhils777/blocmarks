<h1> Your Blocmarks! </h1>


<% @topics_hash.each do |topic, blocmark_list| %>
<table class="table table-bordered">
  <tr class="info">
    <td><%= topic.title %></td>
  </tr>
  <% blocmark_list.each do |bloc| %>
    <tr>
      <td><%= bloc.url %></td>
    </tr>
  <% end %>
</table>
<br>  
<% end %>

